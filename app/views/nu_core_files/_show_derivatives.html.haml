
%h4 Downloads

- if !(@nu_core_file.under_embargo?(current_user))
  .btn-toolbar
    = render partial: 'derivative_btn_group', locals: {contents: @nu_core_file.content_objects, list_view: true, item: @nu_core_file}
- else
  %span= "Downloads available #{@nu_core_file.embargo_release_date(formatted: true)}"
