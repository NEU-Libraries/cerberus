= render :partial => 'breadcrumbs'

%h1 Edit Metadata
%br
= form_for(@nu_core_file, :url => nu_core_file_path, :html => {:multipart => true }) do |f|

  = render :partial => 'nu_core_files/metadata/default', :formats => [:html], :locals => {:f => f, :new_object => false}

  / We need to account for the different file types here to extend the metdata screen

  .form-actions
    = f.submit class: ['btn', 'btn-primary'], :onclick => "confirmation_needed = false;", value: 'Submit'
    = button_tag "Reset", class: 'btn', type: :reset

