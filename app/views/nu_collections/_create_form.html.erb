<%= form_for :nu_collection, url: nu_collections_path do |f| %>
  <p>
    <%= f.label :nu_title, "Nu Title" %><br>
    <%= f.text_field :nu_title %>
  </p>
 
  <p>
    <%= f.label :nu_description, "Nu Description" %><br>
    <%= f.text_area :nu_description %>
  </p>

  <p>
    <%= f.label :mods_abstract, "Describe your collection's purpose here:" %><br>
    <%= f.text_area :mods_abstract %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>