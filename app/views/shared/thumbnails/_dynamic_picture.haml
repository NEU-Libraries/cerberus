- unless defined? thumbs 
  - thumbs = @nu_core_file.thumbnail_list


-if thumbs.length > 0
  %span{"data-alt" => "", "data-picture" => ""}    
    %span.thumbnail-s1{"data-src" => thumbs[0] , "data-media" => "(min-width: 0)", "data-active" => "false" }
      /Fallback content for non-JS browsers. Same img src as the initial, unqualified source element.
    %noscript
      %img{alt: " ", src: thumbs[0]}
    / - if thumbs['thumbnail_1_2x']
    /   %span.thumbnail-s1-2x{"data-src" => thumbs['thumbnail_1_2x'] , "data-media" => "(min-device-pixel-ratio: 2.0), ( -webkit-min-device-pixel-ratio: 2.0 ) ", "data-active" => "false" } 
    - if thumbs.length > 1
      %span.thumbnail-s2{"data-src" => thumbs[1], "data-media" => "(min-width: 600px)", "data-active" => "false" }
    - if thumbs.length > 2
      %span.thumbnail-s2-2x{"data-src" => thumbs[2], "data-media" => "(min-width: 600px) and (min-device-pixel-ratio: 2.0), (min-width: 600px) and (-webkit-min-device-pixel-ratio: 2.0)" , "data-active" => "false" }
    - if thumbs.length > 3
      %span.thumbnail-s4{"data-src" => thumbs[3], "data-media" => "false", "data-active" => "(min-width: 0)"}
    - if thumbs.length > 4
      %span.thumbnail-s4-2x{"data-src" => thumbs[4], "data-media" => "false" , "data-active" => "(min-width: 400px) and (min-device-pixel-ratio: 2.0), (min-width: 400px) and (-webkit-min-device-pixel-ratio: 2.0)"}
    - if thumbs.length > 5
      %span.thumbnail-s10{"data-src" => thumbs[5],"data-media" => "false" ,  "data-active" => "(min-width: 700px)"}
    - if thumbs.length > 6
      %span.thumbnail-s10-2x{"data-src" => thumbs[6], "data-media" => "false" ,  "data-active" => "(min-width: 700px) and (min-device-pixel-ratio: 2.0), (min-width: 700px) and (-webkit-min-device-pixel-ratio: 2.0)"}