  <entry xmlns:thr="http://purl.org/syndication/thread/1.0">
    <id>info:fedora/demo:25/WSDL/2008-07-02T05:09:42.859Z</id>
    <title type="text">WSDL1.0</title>
    <updated>2008-07-02T05:09:42.859Z</updated>
    <thr:in-reply-to ref="info:fedora/demo:25/WSDL"/>
    <category scheme="info:fedora/fedora-system:def/model#formatURI" term="http://schemas.xmlsoap.org/wsdl/"/>
    <category scheme="info:fedora/fedora-system:def/model#label" term="WSDL Bindings"/>
    <content type="text/xml">
      <wsdl:definitions name="FedoraLocalSaxonService" targetNamespace="saxon"
        xmlns:http="http://schemas.xmlsoap.org/wsdl/http/"
        xmlns:mime="http://schemas.xmlsoap.org/wsdl/mime/"
        xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap"
        xmlns:soapenc="http://schemas.xmlsoap.org/wsdl/soap/encoding"
        xmlns:this="saxon" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
        <wsdl:types>
          <xsd:schema targetNamespace="saxon">
            <xsd:simpleType name="URLType">
              <xsd:restriction base="xsd:string"/>
            </xsd:simpleType>
          </xsd:schema>
        </wsdl:types>
        <wsdl:message name="getFORequest">
          <wsdl:part name="TEI_SOURCE" type="this:URLType"/>
        </wsdl:message>
        <wsdl:message name="response">
          <wsdl:part name="fo" type="xsd:base64Binary"/>
        </wsdl:message>
        <wsdl:portType name="FedoraSaxonPortType">
          <wsdl:operation name="getFO">
            <wsdl:input message="this:getFORequest"/>
            <wsdl:output message="this:response"/>
          </wsdl:operation>
        </wsdl:portType>
        <wsdl:service name="FedoraSaxon">
          <wsdl:port binding="this:FedoraSaxon_http" name="FedoraSaxon_port">
            <http:address location="http://local.fedora.server/saxon/"/>
          </wsdl:port>
        </wsdl:service>
        <wsdl:binding name="FedoraSaxon_http" type="this:FedoraSaxonPortType">
          <http:binding verb="GET"/>
          <wsdl:operation name="getFO">
            <http:operation location="SaxonServlet?source=(TEI_SOURCE)&amp;style=http://local.fedora.server/fedora-demo/formatting-objects-demo/fo/tei.xsl"/>
            <wsdl:input>
              <http:urlReplacement/>
            </wsdl:input>
            <wsdl:output>
              <mime:content type="text/xml"/>
            </wsdl:output>
          </wsdl:operation>
        </wsdl:binding>
      </wsdl:definitions>
    </content>
  </entry>
