    <category scheme="info:fedora/fedora-system:def/model#label" term="Datastream Input Specification"/>
    <content type="text/xml">
      <fbs:DSInputSpec label="Mapping to the Fedora Local Saxon Service" xmlns:fbs="http://fedora.comm.nsdlib.org/service/bindspec">
        <fbs:DSInput DSMax="1" DSMin="1" DSOrdinality="false" wsdlMsgPartName="TEI_SOURCE">
          <fbs:DSInputLabel>TEI source file</fbs:DSInputLabel>
          <fbs:DSMIME>text/xml</fbs:DSMIME>
          <fbs:DSInputInstruction>XML source file in TEI format, to be transformed to FO format
                        </fbs:DSInputInstruction>
        </fbs:DSInput>
      </fbs:DSInputSpec>
