        <METS:xmlData>
          <fbs:DSInputSpec label="Mapping to the Fedora Local FOP Service" xmlns:fbs="http://fedora.comm.nsdlib.org/service/bindspec">
            <fbs:DSInput DSMax="1" DSMin="1" DSOrdinality="false" wsdlMsgPartName="XML_SOURCE">
