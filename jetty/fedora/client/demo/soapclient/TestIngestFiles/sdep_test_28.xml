    <foxml:datastream CONTROL_GROUP="X" ID="RELS-EXT" STATE="A" VERSIONABLE="true">
        <foxml:datastreamVersion ID="RELS-EXT1.0" LABEL="Relationships" MIMETYPE="application/rdf+xml">
