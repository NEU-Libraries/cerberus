	<!-- ****************************************************************** -->
	<fbm:modifyDatastream pid="demo:32" dsID="DC" dsControlGroupType="X" dsLabel="New label for DC datastream" checksumType="MD5" logMessage="BatchModify - modifyDatastream">
		<fbm:xmlData>
